{
  "version": 2,
  "builds": [{
    "src": "package.json",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "build",
      "includeFiles": [
        "src/**",
        "public/**",
        "package.json"
      ]
    }
  }],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://8.138.248.160:3001/api/$1"
    },
    {
      "src": "/(.*)", 
      "dest": "/index.html"
    }
  ],
  "regions": ["hkg1"]
}
